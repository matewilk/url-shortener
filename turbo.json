{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "check:types": {
      "dependsOn": ["^check:types"]
    },
    "dev": {
      "persistent": true,
      "cache": false,
      "dependsOn": ["dev:db:start"]
    },
    "dev:db:start": {},
    "unit:test": {
      "dependsOn": ["^unit:test"]
    },
    "int:test": {
      "dependsOn": ["^int:test"]
    },
    "e2e:test": {
      "dependsOn": ["^e2e:test"]
    }
  }
}
